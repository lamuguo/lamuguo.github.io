{
  "type": "AdaptiveCard",
  "body": [
    {
      "type": "ColumnSet",
      "columns": [
        {
          "type": "Column",
          "items": [
            {
              "type": "TextBlock",
              "text": "Coverage report",
              "wrap": true,
              "size": "small",
              "weight": "bolder",
              "isSubtle": false,
              "spacing": "small"
            },
            {
              "type": "TextBlock",
              "text": "${cov_url}",
              "wrap": true,
              "size": "Small",
              "weight": "Lighter",
              "isSubtle": true,
              "spacing": "Small"
            },
            {
              "type": "Column",
              "items": [
                {
                  "type": "TextBlock",
                  "text": "Lint report",
                  "wrap": true,
                  "size": "small",
                  "weight": "bolder",
                  "isSubtle": false,
                  "spacing": "small"
                },
                {
                  "type": "TextBlock",
                  "text": "${lint_url}",
                  "wrap": true,
                  "size": "Small",
                  "weight": "Lighter",
                  "isSubtle": true,
                  "spacing": "Small"
                }
          ],
          "width": "stretch"
        }
      ],
      "style": "default"
    }
  ],
  "actions": [
    {
      "type": "Action.OpenUrl",
      "title": "Coverage report",
      "url": "${cov_url}"
    },
    {
      "type": "Action.OpenUrl",
      "title": "Lint report",
      "url": "${lint_url}"
    }
  ],
  "$schema": "http://adaptivecards.io/schemas/adaptive-card.json",
  "version": "1.4"
}